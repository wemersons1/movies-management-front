    "apexcharts": "^3.45.2",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "firebase": "^10.8.0",
    "moment": "^2.30.1",
    "nanoid": "^5.0.5",
    "react": "^18",
